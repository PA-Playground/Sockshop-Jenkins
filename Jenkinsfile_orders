node {

   stage('Clone orders.NET from github') {

       git 'https://github.com/PA-Playground/orders.NET.git'

   }

   stage('Build and deploy orders.NET') {

       sh "docker build -f docker/orders.net/Dockerfile ."

   }
   
   stage('Push to AWS repo') {
   
       sh "docker tag orders-net-repo:latest 826081382146.dkr.ecr.eu-west-1.amazonaws.com/orders-net-repo:latest"
   }

}
